            <MenuItem fx:id="miSave" mnemonicParsing="false" onAction="#saveGame" text="%save"/>
            <MenuItem fx:id="miSaveAs" mnemonicParsing="false" onAction="#saveGameAs" text="%saveAs"/>
            <SeparatorMenuItem mnemonicParsing="false"/>
            <MenuItem fx:id="miQuit" mnemonicParsing="false" onAction="#quit" text="%quit"/>
        </Menu>
        <Menu mnemonicParsing="false" text="%edit">
            <Menu mnemonicParsing="false" text="%preferences">
                <MenuItem fx:id="miSymbols" mnemonicParsing="false" onAction="#editSymbols" text="%symbols"/>
                <Menu mnemonicParsing="false" text="%language">
                    <MenuItem fx:id="en" mnemonicParsing="false" onAction="#editLanguage" text="%english" />
                    <MenuItem fx:id="it" mnemonicParsing="false" onAction="#editLanguage" text="%italian" />
                </Menu>
            </Menu>
        </Menu>
        <Menu mnemonicParsing="false" text="%help">
            <MenuItem fx:id="miAbout" mnemonicParsing="false" onAction="#showAbout" text="%about"/>
        </Menu>
    </MenuBar>
    <!-- END MENU -->
