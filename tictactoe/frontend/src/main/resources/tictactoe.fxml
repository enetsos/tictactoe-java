    <!-- MENU -->
    <MenuBar prefHeight="29.0" prefWidth="600.0">
        <Menu mnemonicParsing="false" text="%file">
            <MenuItem fx:id="miNew" mnemonicParsing="false" onAction="#newGame" text="%new"/>
            <MenuItem fx:id="miOpen" mnemonicParsing="false" onAction="#openGame" text="%open"/>
